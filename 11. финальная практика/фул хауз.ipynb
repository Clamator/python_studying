    "def is_full_house(lst):\n",
    "    my_dict = {i:lst.count(i) for i in lst}\n",
    "    \n",
    "    num = 0\n",
    "    for k, v in my_dict.items():\n",
    "        if v >=2:\n",
    "            num +=1\n",
    "    if num >= 2:\n",
    "        return True\n",
    "    else:\n",
    "        return False"
